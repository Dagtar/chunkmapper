<launch4jConfig>
  <headerType>gui</headerType>
  <jar>Chunkmapper.jar</jar>
  <outfile>Chunkmapper.exe</outfile>
  <errTitle>Chunkmapper</errTitle>
  <chdir>.</chdir>
  <icon>Globe.ico</icon>
  <jre>
    <minVersion>1.6.0</minVersion>
    <maxHeapSize>1024</maxHeapSize>
  </jre>
  <splash>
    <file>Globe_Splash.bmp</file>
    <waitForWindow>true</waitForWindow>
    <timeout>60</timeout>
    <timeoutErr>true</timeoutErr>
  </splash>
</launch4jConfig>
